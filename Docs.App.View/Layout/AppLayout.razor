@namespace Skclusive.Material.Docs.App.View
@inherits LayoutComponentBase

<MainLayout
    TopbarStyle="color: var(--theme-palette-background-custom-appbar-color); background-color: var(--theme-palette-background-custom-appbar-background-color);">
    <LogoContent>
        <AppLogo />
    </LogoContent>
    <TitleContent>
        <AppTitle />
    </TitleContent>
    <ActionsContent>
        <AppActions />
    </ActionsContent>
    <BodyContent>
        <AppBody>
            @Body
        </AppBody>
    </BodyContent>
     <SidebarContent>
        <AppSidebar />
    </SidebarContent>
</MainLayout>